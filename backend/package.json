{
  "name": "backend",
  "version": "1.0.0",
  "description": "Backend con Express, TypeScript y soporte para usuarios, fotos y comentarios",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "echo \"No test configured\" && exit 0",
    "test-db": "ts-node src/testDB.ts",
    "check-env": "ts-node src/check-env.ts",
    
    
    "model-server": "cd scripts && python modelo_server.py",
    "model-server-simple": "cd scripts && python modelo_server_simple.py",
    "install-python-deps": "cd scripts && pip install -r requirements_optimizado.txt",
    "dev-with-models": "concurrently \"npm run model-server\" \"npm run dev\"",
    "dev-full": "concurrently \"npm run model-server\" \"npm run dev\"",
    
    
    "start-with-models": "concurrently \"npm run model-server\" \"npm run start\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
